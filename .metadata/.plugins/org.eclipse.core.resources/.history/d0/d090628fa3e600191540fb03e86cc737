package com.string.java;

public class RemoveDuplicat {
	
	public static void main(String[] args) {
		String str1="abcdabcd";
		
		int i,j, k = 0;
		for ( i = 0; i < str1.length(); i++) {
			for (j = 0; j < i; j++) {
				if (str1[i] == str1[j]) {
					break;
				}
			}
			if (str1[i] == str1[j]) {
				str1[k++] = str1[i];
			}
	}
	}
}

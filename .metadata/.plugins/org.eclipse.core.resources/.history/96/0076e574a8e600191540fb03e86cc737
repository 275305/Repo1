package com.string.java;

public class Stringpalindrome {

	public static void main(String[] args) {
  int i,j;
		String strRev = "abcd";
		char arr[] = strRev.toCharArray();
		int len = arr.length;
		 int len1  =len/2;
		for (i = 0; i < len1; i++) {
		 for(j=len-1;j<len1;j--){
			
			if(arr[i]==arr[j]){
			System.out.println(arr[i]);
			}
		}
	}
	}
}

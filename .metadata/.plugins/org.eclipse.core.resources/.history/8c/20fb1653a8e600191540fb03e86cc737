package com.string.java;

public class Stringpalindrome {

	public static void main(String[] args) {
  int i,j;
		String strRev = "abcd";
		char arr[] = strRev.toCharArray();
		int len = arr.length;
		 int len1  =len/2;
		for (i = len - 1; i < len1; i--) {
			while()
			
			if(arr[i]==arr[j]){
			System.out.println(arr[i]);
			}
		}
	}

}
